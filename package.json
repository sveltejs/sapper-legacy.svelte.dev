{
  "name": "sapper.svelte.technology",
  "description": "The Sapper website",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "start": "serve export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "stage": "now export",
    "prestage": "npm run build",
    "build": "sapper export",
    "deploy": "git-branch-is master && npm run stage && now alias",
    "deploy:ci": "now export --token $NOW_TOKEN && now alias --token $NOW_TOKEN",
    "predeploy:ci": "sapper export"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "highlight.js": "^9.12.0",
    "marked": "^0.4.0",
    "node-fetch": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "sapper": "^0.14.2",
    "serve": "^9.1.0",
    "serve-static": "^1.13.1",
    "svelte": "^2.8.1",
    "svelte-loader": "^2.9.1",
    "webpack": "^4.13.0"
  },
  "now": {
    "alias": "sapper.svelte.technology"
  },
  "devDependencies": {
    "git-branch-is": "^1.0.0",
    "now": "^11.2.4"
  }
}
