{
  "name": "sapper.svelte.technology",
  "description": "The Sapper website",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "start": "serve __sapper__/export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "stage": "now export",
    "prestage": "npm run build",
    "build": "sapper export",
    "deploy": "git-branch-is master && npm run stage && now alias",
    "deploy:ci": "now __sapper__/export --token $NOW_TOKEN && now alias --token $NOW_TOKEN",
    "predeploy:ci": "sapper export"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "highlight.js": "^9.12.0",
    "marked": "^0.7.0",
    "node-fetch": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "serve": "^9.4.0",
    "serve-static": "^1.13.1",
    "svelte": "^2.9.11"
  },
  "now": {
    "alias": "sapper.svelte.technology"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "git-branch-is": "^2.0.0",
    "now": "^11.3.8",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^4.3.1",
    "rollup-plugin-terser": "^3.0.0",
    "sapper": "^0.23.1"
  }
}
